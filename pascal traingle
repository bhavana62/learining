import java.util.*;

public class RightTrianglePattern {
public static void main(String args[]) {

Scanner in = new Scanner(System.in);
System.out.println("Enter number of rows in pattern");
int no = in.nextInt();
int number=1;

for(int i = 0; i < no; i++) {
    number=1;
for(int j = 0; j <= i; j++) {
System.out.print(number +" ");
number=number*((i-j)/(j+1));

}
System.out.print("\n");
}
}
}
